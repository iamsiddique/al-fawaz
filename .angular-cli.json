{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "al-fawaz"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css","assets/plugins/bootstrap/bootstrap.min.css","assets/plugins/ionicons/ionicons.min.css","assets/plugins/animate-css/animate.css","assets/plugins/slider/slider.css",
        "assets/plugins/owl-carousel/owl.carousel.css","assets/plugins/owl-carousel/owl.theme.css","assets/plugins/facncybox/jquery.fancybox.css","assets/css/style.css",
        "assets/css/main.css","assets/css/responsive.css"

      ],
      "scripts": ["assets/plugins/jQurey/jquery.min.js","assets/plugins/form-validation/jquery.form.js","assets/plugins/form-validation/jquery.validate.min.js","assets/plugins/owl-carousel/owl.carousel.min.js","assets/plugins/bootstrap/bootstrap.min.js",
        "assets/plugins/wow-js/wow.min.js","assets/plugins/slider/slider.js","assets/plugins/facncybox/jquery.fancybox.js","assets/js/main.js"],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
